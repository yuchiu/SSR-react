{
  "name": "test-repo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "webpack:client": "webpack --config webpack.client.config.js --watch",
    "webpack:server": "webpack --config webpack.server.config.js --watch",
    "webpack:start": "nodemon --watch build --exec node build/bundle.js",
    "start": "npm-run-all --parallel webpack:*",
    "build": "webpack --config webpack.client.config.js && webpack --config webpack.server.config.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuchiu/test-repo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yuchiu/test-repo/issues"
  },
  "homepage": "https://github.com/yuchiu/test-repo#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.7",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "class-methods-use-this": 0,
      "import/prefer-default-export": 0,
      "no-underscore-dangle": 0,
      "linebreak-style": 0,
      "no-unused-vars": 0,
      "no-console": 0,
      "import/no-unresolved": 0,
      "import/no-cycle": 0,
      "no-plusplus": 0,
      "no-use-before-define": 0,
      "camelcase": 0,
      "prefer-destructuring": 0,
      "allowNamedFunctions": true,
      "import/no-mutable-exports": 0,
      "no-unused-expressions": 0
    }
  }
}
